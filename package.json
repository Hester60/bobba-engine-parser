{
  "name": "@bobba-engine/parser",
  "version": "1.1.3",
  "description": "Bobba engine tilemap parser inspired by Habbo rooms with the help of Shroom package.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "habbo",
    "tilemap",
    "parser",
    "bobba",
    "engine",
    "room",
    "shroom",
    "isometric"
  ],
  "license": "MIT",
  "author": "CARBUCCIA Thomas",
  "repository": {
    "type": "git",
    "url": "https://github.com/Hester60/bobba-engine-parser.git"
  },
  "bugs": {
    "url": "https://github.com/Hester60/bobba-engine-parser/issues"
  },
  "homepage": "https://github.com/Hester60/bobba-engine-parser#readme",
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
